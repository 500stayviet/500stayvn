{
    "version": 1,
    "framework": { "name": "nextjs", "version": "15.1.0" },
    "imageSettings": { "dangerouslyAllowSVG": false },
    "routes": [
      {
        "path": "/_next/static/*",
        "target": { "kind": "Static", "cacheControl": "public, max-age=31536000, immutable" }
      },
      {
        "path": "/*",
        "target": { "kind": "Compute", "src": "default" }
      }
    ],
    "computeResources": [
      {
        "name": "default",
        "runtime": "nodejs20.x",
        "entrypoint": "server.js"
      }
    ]
  }